{
  "name": "castle-falkenstein",
  "version": "0.3.0",
  "title": "Castle Falkenstein",
  "description": "FoundryVTT system for Castle Falkenstein.",
  "minimumCoreVersion": "9",
  "compatibleCoreVersion": "9",
  "authors": [ "admiralnlson" ],
  "esmodules": [
    "src/lib/socketlib/socketlib.js",
    "src/castle-falkenstein.mjs"
  ],
  "styles": ["src/css/castle-falkenstein.css"],
  "packs": [
    {
      "name": "abilities-corebook",
      "label": "Abilities - Corebook",
      "path": "build/packs/abilities-corebook.db",
      "type": "Item"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "fr",
      "name": "French",
      "path": "lang/fr.json"
    }
  ],
  "dependencies": [ 
    {
      "name": "monarch",
      "type": "module",
      "manifest": "https://github.com/zeel01/monarch/releases/latest/download/module.json"
    },
    {
      "name": "lib-wrapper",
      "type": "module",
      "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json"
    }
  ],
  "socket": true,
  "manifest": "https://github.com/admiralnlson/castle-falkenstein-fvtt/releases/latest/download/system.json",
  "url": "https://github.com/admiralnlson/castle-falkenstein-fvtt",
  "download": "https://github.com/admiralnlson/castle-falkenstein-fvtt/releases/latest/download/castle-falkenstein.zip",
  "license": "https://github.com/admiralnlson/castle-falkenstein-fvtt/blob/master/LICENSE",
  "readme": "https://github.com/admiralnlson/castle-falkenstein-fvtt/blob/master/README.md",
  "bugs": "https://github.com/admiralnlson/castle-falkenstein-fvtt/issues",
  "changelog": "https://github.com/admiralnlson/castle-falkenstein-fvtt/releases"
}
